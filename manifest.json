{
  "name": "Netflix Roulette",
  "version": "1.0",
  "description": "Get a random video on netflix",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "permissions": ["tabs"],
  "browser_action": {},
  "manifest_version": 2
}
